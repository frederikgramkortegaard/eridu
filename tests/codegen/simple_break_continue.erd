
int i;
print("Testing break");

i = 10;
while (i > 0) {
    print(i);
    if (i == 5) {
        print("Trying to break from loop");
        break;
    };
    i = i - 1;
};

print("value of i after break:");
print(i);
print("");

print("Testing continue");

i = 10;
while (i != 0) {
    i = i - 1;
    if (i == 6) {
        print("Trying to continue in loop");
        continue;
    };
    print(i);
};